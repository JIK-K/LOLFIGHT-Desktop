  "main": "public/main.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@hasagi/core": "^0.4.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dotenv": "^16.4.5",
    "https-browserify": "^1.0.0",
    "league-connect": "^6.0.0-rc12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "react-scripts": "5.0.1",
    "sweetalert2": "^11.10.5",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "BROWSER=none yarn start",
    "react-start-window": "set BROWSER=none && yarn start --port 5000",
    "electron-start": "ELECTRON_START_URL=http://localhost:5000 electron .",
    "electron-start-window": "set ELECTRON_START_URL=http://localhost:5000 && electron .",
    "electron": "electron .",
    "electron-pack": "yarn build && electron-builder build -c.extraMetadata.main=build/Main.js",
    "dev": "concurrently -k \"npm start\" \"npm:electron\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
